{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "nestjs-starter",
  "author": "Baianat",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^16 || ^18"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test node --max-old-space-size=8192 --expose-gc ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit --logHeapUsage --verbose --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --detectOpenHandles --forceExit --logHeapUsage --verbose --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "preseed": "rimraf dist && nest build",
    "seed": "node ./dist/_common/database/seeders",
    "migrate": "bash ./migrate.sh",
    "migration:new": "cross-env-shell sequelize migration:generate --name $MIG_NAME"
  },
